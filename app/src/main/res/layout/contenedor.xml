<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:navigation="http://schemas.android.com/apk/res-auto"
    android:id="@+id/listing_navigation_drawer"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:background="@color/background_material_dark">
<LinearLayout
    android:orientation="vertical"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <include layout="@layout/toolbar"/>


    <FrameLayout
        android:id="@+id/activity_detalles_fldetalles"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></FrameLayout>

    </LinearLayout>

<android.support.design.widget.NavigationView
    android:id="@+id/list_navigation_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="start"
    navigation:headerLayout="@layout/list_navigation_header"
    navigation:menu="@menu/listing_navegation_menu">
</android.support.design.widget.NavigationView>
</android.support.v4.widget.DrawerLayout>